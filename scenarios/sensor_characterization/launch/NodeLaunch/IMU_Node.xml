 <node name="$(env ROS_HOSTNAME)_imu_node" pkg="icarus_rover_v2" type="imu_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="0.0"/>
	    <param name="verbosity_level"       value="DEBUG"/> 
        <param name="require_pps_to_start"  value="false"/>
        <param name="loop1_rate"            value="50.0"/> <!-- publish -->
        <param name="use_calibrationfile"   value="true"/> <!-- If set to true will load characterization file and modify sensor data.  If set to false will output raw sensor data. -->
</node>
