<node name="$(env ROS_HOSTNAME)_database_node" pkg="icarus_rover_v2" type="database_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="0.0"/>
        <param name="verbosity_level"       value="NOTICE"/>
