<node name="$(env ROS_HOSTNAME)_controlgroup_node" pkg="icarus_rover_v2" type="controlgroup_node"  output="screen" clear_params="true">
        <param name="verbosity_level"       value="DEBUG"/>
        <param name="startup_delay"       value="1.0"/> 
        <param name="require_pps_to_start"  value="false"/> 
        <param name="loop1_rate"            value="20.0"/> <!-- Output Publish -->
        <param name="loop2_rate"            value="-1.0"/> 
        <param name="loop3_rate"            value="-1.0"/>
</node>
